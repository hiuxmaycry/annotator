{
  "name": "annotator",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.27",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "eslint-plugin-jest": "26.2.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-scripts": "5.0.1",
    "typescript": "4.6.3",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint",
    "lint:fix": "eslint src --fix",
    "format": "prettier --list-different 'src/**/*.{ts,tsx,js,json}'",
    "format:fix": "prettier --write 'src/**/*.{ts,tsx,js,json}'",
    "tdd": "react-scripts test",
    "unit": "CI=true react-scripts test",
    "build": "react-scripts build",
    "check-types": "tsc --noEmit",
    "test": "npm run lint && npm run format && npm run check-types && npm run unit",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "prettier": "2.6.2"
  }
}
